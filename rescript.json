{
  "name": "rescript-vscode",
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "suffix": ".bs.js",
  "namespace": false,
  "bs-dependencies": [
    "@rescript/core"
  ],
  "bsc-flags": [
    "-open RescriptCore",
    "-bs-g"
  ]
}